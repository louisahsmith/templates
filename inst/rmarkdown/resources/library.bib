issn = {1476-6256},
journal = {American Journal of Epidemiology},
keywords = {*Case-Control Studies,*Selection Bias,Case Control Studies,Case-Control Studies,Confounding Factors (Epidemiology),Effect Modifier,Efficiency,Environmental Exposure,Epidemiologic,Humans,Random Allocation,Reproducibility of Results,Research Design/*standards,Sampling Studies,case-control},
month = {may},
