journal = {Paediatric and perinatal epidemiology},
keywords = {Epidemiologic Methods,Female,Fertilization,Fetal Growth Retardation,Fetal Growth Retardation: epidemiology,Folder - Seasonality Lit Review,Gambia,Gambia: epidemiology,Genotype,Humans,Infant, Newborn,Infant, Small for Gestational Age,Male,Pregnancy,Premature Birth,Premature Birth: epidemiology,Rural Health,Seasons},
