journal = {American Journal of Obstetrics and Gynecology},
keywords = {Adult,African Americans,Birth Weight,Cesarean Section,Ethnic Groups,European Continental Ancestry Group,Female,Hispanic Americans,Humans,Indians,Infant,Mothers,Newborn,North American,Pregnancy,Risk Factors},
