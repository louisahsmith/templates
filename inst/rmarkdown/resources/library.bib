isbn = {0145-2134},
issn = {01452134},
journal = {Child Abuse and Neglect},
keywords = {Child adjustment,Community-based child- and parent group interventi,Domestic violence,RCT,Trauma-focused cognitive behavioral therapy,Treatment differentiation and adherence},
