journal = {Journal of developmental and behavioral pediatrics : JDBP},
number = {2},
pages = {82--91},
pmid = {17435458},
title = {{Effect of supplementation with polyunsaturated fatty acids and micronutrients on learning and behavior problems associated with child ADHD.}},
