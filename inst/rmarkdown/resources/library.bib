issn = {01621459},
journal = {Journal of the American Statistical Association},
keywords = {as if they were,distribution and variance estimation,however,in obtaining variance estimates,item nonresponse,leads to seri-,multistage sampling,random imputation,ratio and,regression imputation,treating the imputed values,true,uniform response},
month = {sep},
number = {435},
pages = {1278--1288},
title = {{Bootstrap for Imputed Survey Data}},
