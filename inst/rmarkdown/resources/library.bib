issn = {1044-3983},
journal = {Epidemiology},
number = {1},
pages = {125--132},
pmid = {26426943},
title = {{Bias Formulas for Estimating Direct and Indirect Effects When Unmeasured Confounding Is Present}},
url = {http://content.wkhealth.com/linkback/openurl?sid=WKPTLP:landingpage{\&}an=00001648-900000000-99126{\%}5Cnhttp://content.wkhealth.com/linkback/openurl?sid=WKPTLP:landingpage{\&}an=00001648-201601000-00018},
volume = {27},
