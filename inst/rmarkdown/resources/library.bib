issn = {2168-6203},
journal = {JAMA Pediatrics},
keywords = {Adolescent,Aggression,Aggression: psychology,Child,Cognition,Emotions,Female,Humans,Longitudinal Studies,Male,Risk Factors,Singapore,Time Factors,Video Games,Video Games: psychology,Violence,Violence: psychology},
number = {5},
pages = {450},
pmid = {24663396},
title = {{Mediators and Moderators of Long-term Effects of Violent Video Games on Aggressive Behavior}},
