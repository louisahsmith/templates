issn = {09652140},
journal = {Addiction},
keywords = {Cocaine,Female,Humans,Infant,Low Birth Weight,Newborn,Pregnancy,Pregnancy Complications,Substance-Related Disorders},
month = {nov},
number = {11},
pages = {1561--1570},
pmid = {9519498},
shorttitle = {Maternal cocaine use and low birth weight newborns},
title = {{Maternal cocaine use and low birth weight newborns: a meta-analysis}},
