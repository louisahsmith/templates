issn = {1044-3983; 1044-3983},
journal = {Epidemiology},
keywords = {Case-Control Studies,Disease/etiology,Environmental Exposure/adverse effects,Epidemiologic Methods,Epidemiologic Research Design,Evaluation Studies as Topic,Humans,Longitudinal Studies,Models,Research,Research Subjects,Risk Factors,Selection Bias,Theoretical},
