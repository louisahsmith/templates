pages = {500--505},
title = {{Quantitative analysis of associations between birth defects and suspected human teratogens}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0025826775{\&}partnerID=tZOtx3y1},
volume = {40},
year = {1991}
}
