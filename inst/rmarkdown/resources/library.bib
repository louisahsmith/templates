file = {:Users/louisahsmith/Box Sync/Mendeley/Lutiger et al/Lutiger et al.{\_}1991{\_}Relationship between gestational cocaine use and pregnancy outcome A meta analysis.pdf:pdf},
isbn = {0040-3709},
