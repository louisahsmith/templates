issn = {02765624},
journal = {Research in Social Stratification and Mobility},
keywords = {College completion,College enrollment,Educational attainment,Educational transitions model,Event history methods,NLSY79,Social stratification},
number = {1},
pages = {23--44},
publisher = {Elsevier Ltd},
title = {{Do all roads lead to Rome? Effect of educational trajectories on educational transitions}},
