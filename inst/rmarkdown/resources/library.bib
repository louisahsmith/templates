issn = {00029262},
journal = {American Journal of Epidemiology},
keywords = {bias (epidemiology),simulation},
number = {10},
pages = {938--948},
pmid = {23100247},
title = {{Implications of m bias in epidemiologic studies: A simulation study}},
volume = {176},
year = {2012}
}
