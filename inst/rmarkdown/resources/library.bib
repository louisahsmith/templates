keywords = {Animals,Body Burden,Dioxins/pharmacokinetics/*toxicity,Disease Models- Animal,Endometriosis/*chemically induced,Environmental Pollutants/pharmacokinetics/*toxicit,Female,Folder - Endometriosis,Humans},
