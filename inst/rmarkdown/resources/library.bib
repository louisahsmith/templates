journal = {American Journal of Human Biology},
keywords = {17th Century,18th Century,Cohort Studies,Environment,Female,Folder - Development Outcomes,History,Human Migration,Humans,Longevity,Mortality,Nutritional Status,Parturition,Phenotype,Proportional Hazards Models,Quebec,Seasons},
language = {en},
