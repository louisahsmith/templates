pages = {7683},
title = {{A longitudinal examination of children's emotion regulation problems, negative parenting behaviors, and the development of internalizing behavior problems.}},
url = {http://gateway.proquest.com/openurl?url{\_}ver=Z39.88-2004{\&}rft{\_}val{\_}fmt=info:ofi/fmt:kev:mtx:dissertation{\&}res{\_}dat=xri:pqdiss{\&}rft{\_}dat=xri:pqdiss:3476432{\%}5Cnhttp://ovidsp.ovid.com/ovidweb.cgi?T=JS{\&}PAGE=reference{\&}D=psyc7{\&}NEWS=N{\&}AN=2012-99120-478},
