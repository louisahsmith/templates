journal = {The International Journal of Biostatistics},
number = {1},
pages = {Article 17},
pmid = {22462152},
title = {{Estimation based on case-control designs with known prevalence probability.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=22462152{\&}retmode=ref{\&}cmd=prlinks},
volume = {4},
year = {2008}
}
