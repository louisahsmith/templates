@article{Rose2008,
abstract = {Researchers of uncommon diseases are often interested in$\backslash$nassessing potential risk factors. Given the low$\backslash$nincidence of disease, these studies are frequently$\backslash$ncase-control in design, as this allows for a$\backslash$nsufficient number of cases to be obtained without$\backslash$nextensive sampling and can increase$\backslash$nefficiency. However, these case-control samples are$\backslash$nthen biased since the proportion of cases in the$\backslash$nsample is not the same as the population of$\backslash$ninterest. Methods for analyzing case-control studies$\backslash$nhave focused on utilizing logistic regression models$\backslash$nthat provide conditional and not causal estimates of$\backslash$nthe odds ratio. This article will demonstrate the$\backslash$nuse of the prevalence probability and case-control$\backslash$nweighted targeted maximum likelihood estimation$\backslash$n(MLE), as described by van der Laan (2008), in order$\backslash$nto obtain causal estimates of the parameters of$\backslash$ninterest (risk difference, relative risk, and odds$\backslash$nratio). It is meant to be used as a guide for$\backslash$nresearchers, with step-by-step directions to$\backslash$nimplement this methodology. We will also present$\backslash$nsimulation studies that show the improved efficiency$\backslash$nof the case-control weighted targeted MLE compared$\backslash$nto other techniques.},
author = {Rose, S and van der Laan, M J},
file = {:Users/louisahsmith/Box Sync/Mendeley/Rose, van der Laan/Rose, van der Laan{\_}2008{\_}A Guide to Causal Parameters in Case-Control Designs Targeted Maximum Likelihood Estimation.pdf:pdf},
journal = {Technical Report 235, Division of Biostatistics, University of California, Berkeley},
title = {{A Guide to Causal Parameters in Case-Control Designs: Targeted Maximum Likelihood Estimation.}},
year = {2008}
}
