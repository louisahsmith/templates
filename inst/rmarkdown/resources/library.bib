journal = {American journal of epidemiology},
keywords = {*Epidemiologic Methods,Case-Control Studies,Endometrial Neoplasms,Endometrial Neoplasms/epidemiology,Endometrial Neoplasms: epidemiology,Epidemiologic Methods,Female,Humans,Logistic Models,Menarche,Odds Ratio,Research Design,Research Design/statistics {\&} numerical data,Research Design: statistics {\&} numerical data},
language = {eng},
mendeley-tags = {*Epidemiologic Methods,Case-Control Studies,Endometrial Neoplasms/epidemiology,Female,Humans,Logistic Models,Menarche,Odds Ratio,Research Design/statistics {\&} numerical data},
