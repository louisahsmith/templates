journal = {Social Science and Medicine},
keywords = {*Cost of Illness,*Developing Countries,*Seasons,Adolescent,Adult,Aged,Agricultural production,Agriculture,Burkina Faso/epidemiology,Child,Female,Financing- Personal/economics,Health Expenditures/statistics {\&} numerical data,Health Status Indicators,Health care costs,Health care utilization,Health expenditures,Humans,Illness perception,Male,Middle Aged,Rural Health/*trends,Seasonality},
