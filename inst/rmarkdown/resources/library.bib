@article{Gart1982,
abstract = {The performance of 3 approximate CI methods for the odds ratio (R) is studied at the 95{\%} level in the unconditional sample space. They are the method proposed by Cornfield, the logit method with 1/2 corrections first considered by Woolf, and the test-based method proposed by Miettinen. Cornfield's method comes closest to attaining the nominal confidence coefficient. The logit method typically has actual confidence coefficients somewhat too lartge with disparate tail areas. The latter is ascribed in part to the enhanced skewness induced by the logit transformation itself. The test-based method has actual coefficients uniformly less than nominal when R not= 1. This underestimation is worse in finite samples than Halperin found it to be asymptotically. Although the Cornfield and test-based methods have the same confidence coefficients for R = 1, the test-based method is more likely to cover distant values of R not= 1 when in fact R =1. It is concluded that Cornfields's method without the continuity correction is the preferred approximate method in the unconditional space as, with the continuity correction, it was previously found to be in the conditional space.},
author = {Gart, J J and Thomas, D G},
