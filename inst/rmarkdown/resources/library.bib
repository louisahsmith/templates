@article{VanderWeele2013a,
annote = {
So-called controlled direct effects (CDE) assess the effect of the exposure on an outcome with the intermediate fixed to a particular level. So-called natural direct effects (NDE) and natural indirect effects (NIE) allow for the decomposition of a total effect (TE) into direct and indirect effect components. (3-5)... When both direct and indirect effects are employed, a measure of the "proportion mediated" (PM) is sometimes calculated as the ratio of the natural indirect effect to the total effect.... This measure in some sense captures how important the pathway through the intermediate is in explaining the actual operation of the effect of the exposure on the outcome. It measures what would happen to the effect of the exposure on the outcome IF WE WERE TO SOMEHOW DISABLE THE PATHWAY FROM THE EXPOSURE TO THE INTERMEDIATE."


"they require, for each person, fixing the mediator to the counterfactual level the person would have had in the absence of exposure). Such effect estimates do not correspond to actual interventiosn we could carry out in practice, and are thus of limited interest from a policy perspective."

"An alternative proportion measure may be of more interest in policy settings. The controlled direct effect fixing the intermediate to level M = m, CDE(m), captures the effect of the exposure on the outcome if the intermediate were set, possibly contrary to fact, to level m. This is an intervention we might hope to be able to carry out in practice.... carry out in practice... We might hope that by intervening on the intermediate we could block a substantial part of the effect of the exposure on the outcome. A proportion measure that could then be used, and that would be of policy relevance, would be the proportion of the effect of the exposure on the outcome that could be eliminated by intervening to set the intermediate to some fixed level m. This would be [TE-CDE(m)]/TE. If this proportion eliminated were large we might try to implement policies to intervene on the intermediate."

The proportion mediated essentially captures what would happen to the effect of the exposure if we were to somehow disable the pathway from the exposure to the intermediate (leaving it to its natural value); in contrast the proportion eliminated measure captures what would happen to the effect of the exposure on the outcome if we were to fix the intermediate to the same fixed value M=m for all persons.},
author = {VanderWeele, Tyler J.},
doi = {10.1097/EDE.0b013e3182781410},
file = {:Users/louisahsmith/Box Sync/Mendeley/VanderWeele/VanderWeele{\_}2013{\_}Policy-Relevant Proportions for Direct Effects.pdf:pdf},
issn = {1044-3983},
journal = {Epidemiology},
language = {en},
month = {jan},
number = {1},
pages = {175--176},
shorttitle = {Policy-Relevant Proportions for Direct Effects},
title = {{Policy-Relevant Proportions for Direct Effects}},
url = {http://content.wkhealth.com/linkback/openurl?sid=WKPTLP:landingpage{\&}an=00001648-201301000-00032},
volume = {24},
year = {2013}
}
@article{Katz2007,
abstract = {This article examined emotion competence in children exposed to domestic violence (DV). It also examined the hypothesis that children's emotional competence mediates relations between DV and children's later difficulties with peers and behavioral adjustment. DV was assessed when children were at the age of five, emotional competence was assessed at the age of 9.5, and peer quality and behavioral adjustment were obtained at the age of 11. Children from homes with greater DV were less aware of their own emotions and more emotionally dysregulated at the age of 9.5. Emotional awareness mediated the relationship between DV at the age of five and children's friendship closeness and internalizing problems at the age of 11. Emotion dysregulation mediated the relationship between DV at the age of five and children's negative peer group interactions, social problems, and internalizing and externalizing problems at the age of 11. Results are discussed in terms of the impact of DV on children's emotional development and the role that different aspects of emotional competence play in children's socio-emotional adjustment. ABSTRACT FROM AUTHOR},
