issn = {01621459},
journal = {Journal of the American Statistical Association},
keywords = {Bayesian statistics,Confounding,Epidemiologie methods,Leukemia,Magnetic fields,Risk assessment,Sensitivity analysis},
number = {461},
pages = {47--54},
title = {{The impact of prior distributions for uncontrolled confounding and response bias: A case study of the relation of wire codes and magnetic fields to childhood leukemia}},
volume = {98},
year = {2003}
}
