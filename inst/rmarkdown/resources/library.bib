title = {{Four Types of Effect Modification: A Classification Based on Directed Acyclic Graphs}},
url = {http://www.jstor.org/stable/20486427{\%}5Cnhttp://www.jstor.org/stable/pdfplus/20486427.pdf?acceptTC=true},
