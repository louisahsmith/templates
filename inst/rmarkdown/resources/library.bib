journal = {Human reproduction (Oxford, England)},
keywords = {Folder - Endometriosis},
language = {en},
mendeley-tags = {Folder - Endometriosis},
month = {may},
number = {5},
pages = {1059--1068},
pmid = {25743783},
title = {{Increased levels of dioxin-like substances in adipose tissue in patients with deep infiltrating endometriosis.}},
url = {http://humrep.oxfordjournals.org/lookup/doi/10.1093/humrep/dev026},
volume = {30},
year = {2015}
}
