@article{Watanabe2004,
abstract = {OBJECTIVE: To examine whether Arg554Lys polymorphism in the gene for aryl hydrocarbon receptor (AHR) and/or Pro185Ala polymorphism in the gene for aryl hydrocarbon receptor repressor (AHRR) constitutes a susceptibility locus for dioxin-related male infertility.$\backslash$n$\backslash$nDESIGN: Association study of male infertility with polymorphisms.$\backslash$n$\backslash$nSETTING: National research institute and university hospitals.$\backslash$n$\backslash$nPATIENT(S): 123 Japanese men with azoospermia or severe oligozoospermia and 112 men with proven fertility.$\backslash$n$\backslash$nINTERVENTION(S): Polymorphism analysis.$\backslash$n$\backslash$nMAIN OUTCOME MEASURE(S): The allele and genotype frequencies between infertile men and controls.$\backslash$n$\backslash$nRESULT(S): The allele and genotype frequencies of the AHR polymorphism were comparable between infertile men and controls. By contrast, although the difference in the allele frequency of the AHRR polymorphism did not reach a significant level, the genotype frequency was statistically significantly different between the two groups of men. Furthermore, the statistical difference became more significant when the frequency was compared between the Pro/Pro genotype and the Pro/Ala plus Ala/Ala genotype.$\backslash$n$\backslash$nCONCLUSION(S): The Pro185Ala polymorphism in AHRR may constitute a susceptibility locus for dioxin-related male infertility. It appears that the negative feedback effect of AHRR on dioxin-related signaling is weaker for the proline allele than for the alanine allele, and that the hypomorphic function of the proline allele exerts a recessive adverse effect on male fertility.},
annote = {
"For individual's susceptibility to dioxins, it is noteworthy that the biological effects of dioxins are mediated by several molecules including the aryl hydrocarbon receptor (AHR), the aryl hydrocarbon receptor nuclear translocator (ARNT), and the aryl hydrocarbon receptor repressor (AHRR) (7-10)."


 

"To date, two informative polymorphisms, Arg554Lys at exon 10 of AHR and Pro185Ala at exon 6 of AHRR, have been identified in the Japanese population, whereas no useful polymorphism has been detected for ARNT in the Japanese population, including the Asp511Asn and Asp517Glu at exon 16 identified in non-Japanese populations (11-13)."

 

used Taqman Minor Groove Binder (MGB) probes on the ABI PRISM 7000 Sequence Detection System

"The results suggest that the Pro185Ala polymorphism in AHRR, rather than the Arg554Lys polymorphism in AHR, may be involved int he susceptibility to dioxin-related male infertility. Although the difference in allele frequency did not reach the level of statistical significance, the genotype frequency was statistically significantly different between infertile men and controls."},
author = {Watanabe, Masanori and Sueoka, Kou and Sasagawa, Isoji and Nakabayashi, Akira and Yoshimura, Yasunori and Ogata, Tsutomu},
doi = {10.1016/j.fertnstert.2004.06.027},
file = {:Users/louisahsmith/Box Sync/Mendeley/Watanabe et al/Watanabe et al.{\_}2004{\_}Association of male infertility with Pro185Ala polymorphism in the aryl hydrocarbon receptor repressor gene implica.pdf:pdf},
