file = {:Users/louisahsmith/Box Sync/Mendeley/Golan et al/Golan et al.{\_}2005{\_}Specific neurodevelopmental damage in mice offspring following maternal inflammation during pregnancy.pdf:pdf},
isbn = {0028-3908 (Print)$\backslash$r0028-3908 (Linking)},
issn = {00283908},
journal = {Neuropharmacology},
keywords = {Cytokine,Hippocampus,Learning,Memory,Morphogenesis,Neurotrophic factors},
language = {en},
month = {may},
number = {6},
pages = {903--917},
pmid = {15829260},
title = {{Specific neurodevelopmental damage in mice offspring following maternal inflammation during pregnancy}},
